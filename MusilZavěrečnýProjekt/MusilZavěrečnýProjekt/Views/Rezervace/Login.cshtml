@{
}

@model UserViewModel

<form asp-action="Login" method="post">
	<input type="text" id="username" placeholder="Username" asp-for="Username" />
	<input type="email" id="email" placeholder="Email" asp-for="Email" />
	<input type="password" id="password" placeholder="Password" asp-for="Password" />

	<button type="submit">Login</button>
</form>

